<div class="field">
    <%= form.label "Event Title" %>
    <%= form.text_field :event_title, placeholder: "Enter Title Event"%>
</div>

<div class="field">
    <%= form.label "Event Location" %>
    <%= form.text_field :location, placeholder: "Enter Location"%>
</div>

<div class="field">
    <%= form.label "Date" %>
    <%= form.date_field :date %>
</div>

<div class="field">
    <%= form.label "Start Time"%>
    <%= form.time_field :start_time %>
</div>

<div class="field">
    <%= form.label "End Time"%>
    <%= form.time_field :end_time %>
</div>

<div class="field">
    <%= form.label "Description"%>
    <%= form.text_area :description %>
</div>

<div class="action">
    <%= form.submit %>
</div>