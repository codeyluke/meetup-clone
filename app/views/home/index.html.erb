<h1>Meetup Clone App</h1>
<p id=”notice”><%= notice %></p>
<% if current_user %>
    Logged in as <%= current_user.email %>.
    <%= link_to "Log Out", logout_path %>
<% else %>
    <%= link_to "Sign Up", signup_path %> or 
    <%= link_to "Log In", login_path %> or 
    <%= link_to "Sign in with Google", "/auth/google_oauth2" %>
<% end %>
<p>Lorem ipsum …</p>

<p>Other Users</p>
<%= link_to "Other Users", users_path %>

<%= link_to "New Event", new_event_path %>

<h3>Event Hosted By You</h3>

<% @events.each do |event| %>
    <%= link_to "#{event.event_title}", event_path(event.id) %> <br>
<% end %>