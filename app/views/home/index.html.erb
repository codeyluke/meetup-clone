<h1>Meetup Clone App</h1>
<p id=”notice”><%= notice %></p>
<% if current_user %>
    Logged in as <%= link_to "#{current_user.first_name} #{current_user.last_name}", user_path(current_user.id) %>
    <%= link_to "Log Out", logout_path %>
<% else %>
    <%= link_to "Sign Up", signup_path %> or 
    <%= link_to "Log In", login_path %> or 
    <%= link_to "Sign in with Google", "/auth/google_oauth2" %>
<% end %>

<h3>Other Users</h3>
<%= link_to "Other Users", users_path %>

<h3>Create Your Own Event</h3>
<%= link_to "New Event", new_event_path %>

<h3>Events Available</h3>
<%= link_to "Other Events", events_path %>